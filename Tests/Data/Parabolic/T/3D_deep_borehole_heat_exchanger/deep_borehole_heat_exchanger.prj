<?xml version="1.0" encoding="ISO-8859-1"?>
<OpenGeoSysProject>
    <mesh>deep_borehole_heat_exchanger.vtu</mesh>
    <geometry>deep_borehole_heat_exchanger.gml</geometry>
    <processes>
        <process>
            <name>HeatTransportBHE</name>
            <type>HEAT_TRANSPORT_BHE</type>
            <integration_order>2</integration_order>
            <process_variables>
                <process_variable>temperature_soil</process_variable>
                <process_variable>temperature_BHE1</process_variable>
            </process_variables>
            <thermal_conductivity_solid>K_s</thermal_conductivity_solid>
            <heat_capacity_solid>Cp_s</heat_capacity_solid>
            <density_solid>rho_s</density_solid>
            <thermal_conductivity_fluid>K_f</thermal_conductivity_fluid>
            <heat_capacity_fluid>Cp_f</heat_capacity_fluid>
            <density_fluid>rho_f</density_fluid>
            <thermal_conductivity_gas>K_g</thermal_conductivity_gas>
            <heat_capacity_gas>Cp_g</heat_capacity_gas>
            <density_gas>rho_g</density_gas>
            <borehole_heat_exchangers>
                <borehole_heat_exchanger>
                    <bhe_type>BHE_TYPE_CXA</bhe_type>
                    <bhe_bound_type>POWER_IN_WATT</bhe_bound_type>
                    <bhe_power_in_watt_value>-3000.0</bhe_power_in_watt_value>
                    <bhe_use_external_therm_resis>false</bhe_use_external_therm_resis>
                    <bhe_length>18.0</bhe_length>
                    <bhe_diameter>0.216</bhe_diameter>
                    <bhe_refrigerant_flow_rate>0.00833</bhe_refrigerant_flow_rate>
                    <bhe_pipe_inner_radius>0.0477</bhe_pipe_inner_radius>
                    <bhe_pipe_outer_radius>0.0831</bhe_pipe_outer_radius>
                    <bhe_pipe_in_wall_thickness>0.00587</bhe_pipe_in_wall_thickness>
                    <bhe_pipe_out_wall_thickness>0.00734</bhe_pipe_out_wall_thickness>
                    <bhe_fluid_longitudinal_dispsion_length>0.001</bhe_fluid_longitudinal_dispsion_length>
                    <bhe_grout_density>2190.0</bhe_grout_density>
                    <bhe_grout_porosity>0.2</bhe_grout_porosity>
                    <bhe_grout_heat_capacity>1735.16</bhe_grout_heat_capacity>
                    <bhe_inner_pipe_wall_thermal_conductivity>0.21</bhe_inner_pipe_wall_thermal_conductivity>
                    <bhe_outer_pipe_wall_thermal_conductivity>1.3</bhe_outer_pipe_wall_thermal_conductivity>
                    <bhe_grout_thermal_conductivity>1.3</bhe_grout_thermal_conductivity>
                    <bhe_switch_off_threshold>0.001</bhe_switch_off_threshold>
                </borehole_heat_exchanger>
            </borehole_heat_exchangers>
            <secondary_variables>
                <secondary_variable type="static" internal_name="heat_flux_x" output_name="heat_flux_x"/>
                <secondary_variable type="static" internal_name="heat_flux_y" output_name="heat_flux_y"/>
                <secondary_variable type="static" internal_name="heat_flux_z" output_name="heat_flux_z"/>
            </secondary_variables>
            <fluid>
                <refrigerant_density>
                    <type>Constant</type>
                    <value> 1000.0 </value>
                </refrigerant_density>
                <refrigerant_viscosity>
                    <type>Constant</type>
                    <value> 0.00114 </value>
                </refrigerant_viscosity>
                <refrigerant_specific_heat_capacity>
                    <type>Constant</type>
                    <value> 4190.0 </value>
                </refrigerant_specific_heat_capacity>
                <refrigerant_thermal_conductivity>
                    <type>Constant</type>
                    <value> 0.59 </value>
                </refrigerant_thermal_conductivity>
            </fluid>
        </process>
    </processes>
    <time_loop>
        <processes>
            <process ref="HeatTransportBHE">
                <nonlinear_solver>basic_picard</nonlinear_solver>
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <reltol>1.e-6</reltol>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <output>
                    <variables>
                        <variable>temperature_soil</variable>
                        <variable>temperature_BHE1</variable>
                    </variables>
                </output>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial> 0.0 </t_initial>
                    <t_end> 10368000 </t_end>
                    <timesteps>
                        <pair>
                            <repeat>60</repeat>
                            <delta_t>60</delta_t>
                        </pair>
                        <pair>
                            <repeat>167</repeat>
                            <delta_t>3600</delta_t>
                        </pair>
                        <pair>
                            <repeat>64</repeat>
                            <delta_t>21600</delta_t>
                        </pair>
                        <pair>
                            <repeat>10</repeat>
                            <delta_t>43200</delta_t>
                        </pair>
                        <pair>
                            <repeat>92</repeat>
                            <delta_t>86400</delta_t>
                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>deep_borehole_heat_exchanger</prefix>
            <timesteps>
                <pair>
                    <repeat> 10</repeat>
                    <each_steps> 1 </each_steps>
                </pair>
                <pair>
                    <repeat> 10 </repeat>
                    <each_steps> 20 </each_steps>
                </pair>
                <pair>
                    <repeat> 1 </repeat>
                    <each_steps> 393 </each_steps>
                </pair>
            </timesteps>
        </output>
    </time_loop>
    <parameters>
        <parameter>
            <name>K_s</name>
            <type>Constant</type>
            <value>2.5</value>
        </parameter>
        <parameter>
            <name>Cp_s</name>
            <type>Constant</type>
            <value>2000.0</value>
        </parameter>
        <parameter>
            <name>rho_s</name>
            <type>Constant</type>
            <value>1120.0</value>
        </parameter>
        <parameter>
            <name>K_f</name>
            <type>Constant</type>
            <value>0.59</value>
        </parameter>
        <parameter>
            <name>Cp_f</name>
            <type>Constant</type>
            <value>4190.0</value>
        </parameter>
        <parameter>
            <name>rho_f</name>
            <type>Constant</type>
            <value>1000.0</value>
        </parameter>
        <parameter>
            <name>K_g</name>
            <type>Constant</type>
            <value>3.2</value>
        </parameter>
        <parameter>
            <name>Cp_g</name>
            <type>Constant</type>
            <value>1000</value>
        </parameter>
        <parameter>
            <name>rho_g</name>
            <type>Constant</type>
            <value>2500</value>
        </parameter>
        <parameter>
            <name>T0</name>
            <type>Constant</type>
            <value>294.15</value>
        </parameter>
        <parameter>
            <name>T0_BHE1</name>
            <type>Constant</type>
            <values>294.15 294.15 294.15</values>
        </parameter>
    </parameters>
    <process_variables>
        <process_variable>
            <name>temperature_soil</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>T0</initial_condition>
            <boundary_conditions>
            </boundary_conditions>
        </process_variable>
        <process_variable>
            <name>temperature_BHE1</name>
            <components>3</components>
            <order>1</order>
            <initial_condition>T0_BHE1</initial_condition>
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_picard</name>
            <type>Picard</type>
            <max_iter>20</max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
            <lis>-i cg -p jacobi -tol 1e-16 -maxiter 10000</lis>
            <eigen>
                <solver_type>BiCGSTAB</solver_type>
                <precon_type>ILUT</precon_type>
                <max_iteration_step>1000</max_iteration_step>
                <error_tolerance>1e-16</error_tolerance>
            </eigen>
            <petsc>
                <prefix>gw</prefix>
                <parameters>-gw_ksp_type cg -gw_pc_type bjacobi -gw_ksp_rtol 1e-16 -gw_ksp_max_it 10000</parameters>
            </petsc>
        </linear_solver>
    </linear_solvers>
</OpenGeoSysProject>
